{
  "name": "novasystem_modern_ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 3000",
    "dev:flask": "concurrently \"npm run dev\" \"cd .. && python3 -m novasystem.ui.web\"",
    "build": "next build --turbopack",
    "start": "next start --port 3000",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
